{
  "name": "@kronos-integration/service-ldap",
  "version": "0.0.0-semantic-release",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "main": "src/service-ldap.mjs",
  "module": "src/service-ldap.mjs",
  "description": "ldap auth service",
  "keywords": [
    "ldap"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/service-ldap.mjs --section=API",
    "lint": "documentation lint src/service-ldap.mjs",
    "test": "ava"
  },
  "dependencies": {
    "@kronos-integration/service": "^7.1.26",
    "ldapts": "^1.10.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "c8": "^6.0.1",
    "documentation": "^12.1.4",
    "esm": "^3.2.25",
    "semantic-release": "^15.13.31"
  },
  "engines": {
    "node": ">=12.13.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-ldap.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-ldap/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-ldap#readme",
  "ava": {
    "require": [
      "esm"
    ],
    "extensions": [
      "mjs"
    ],
    "babel": false,
    "compileEnhancements": false,
    "files": [
      "tests/*-test.mjs"
    ]
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template-esm-only.git"
    }
  },
  "sideEffects": false
}
