{
  "name": "@kronos-integration/service-ldap",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/service-ldap.mjs"
  },
  "description": "ldap auth service",
  "keywords": [
    "kronos-service",
    "ldap"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "@kronos-integration/service": "^10.4.19",
    "ldapts": "^3.2.1",
    "model-attributes": "^4.1.12"
  },
  "devDependencies": {
    "@kronos-integration/test-interceptor": "^7.0.19",
    "ava": "^4.0.1",
    "c8": "^7.11.0",
    "documentation": "^13.2.5",
    "ldif": "^0.5.1",
    "semantic-release": "^19.0.2"
  },
  "engines": {
    "node": ">=16.13.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/service-ldap.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/service-ldap/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/service-ldap#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-kronos-component"
    ]
  }
}
